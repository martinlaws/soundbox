<p id="notice"><%= notice %></p>

<h3 class="box-title"><%= @box.name %></h3>

<p>
  <strong>User:</strong>
  <%= @current_user.username %>
</p>

<table>
  <tr>
    <th>Track ID</th>
    <th>Track Info</th>
    <th>Created at:</th>
    <th>Updated at:</th>
  </tr>
  <% @box.tracks.each do |track| %>
  <tr>
      <td><%= track.id %></td>
      <td><a href="<%= track.url %>"><%= track.track_info %></a></td>
      <td><%= track.created_at %></td>
      <td><%= track.updated_at %></td>
  </tr>
  <% end %>
</table>
