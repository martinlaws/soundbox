<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row">
    <div class="eight columns">
      <p><strong>Welcome back, </strong><%= @user.username %></p>
    </div>
    <% @user.boxes.each do |box| %>
    <div class ="row">
      <h5><%= box.name %></h5>
    <% box.tracks.each do |track| %>
      <%= track.title %>
      <a href="<%= track.url %>">link</a>
    <% end %>
    </div>
    <% end %>
  </div>

  <div class="row">
    <%= link_to 'Edit', edit_user_path(@user) %>
  </div>
</div>
